from math import sqrt
a = float(input("Введите число а"))
b = float(input("Введите число b"))
c = float(input("Введите число c"))
print("Уравнение вида: ", a, "x^2 + ", b, "x + ", c)
D = (b * 2 - 4 * a * c)
print(D)
b = -(b)
if D > 0:
    x1 = b - (sqrt(D) / (2 * a))
    x2 = b + (sqrt(D) / (2 * a))
    print(x1, x2)
elif D=0:
   X = b / (2 * a)
   print(X)
else:
    print('Корней нет')
